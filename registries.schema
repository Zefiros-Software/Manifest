{
    "type": "array",
    "items": {
        "type" : "object",
        "maxProperties": 2,
        "properties" : {
            "name": {
                "type": "string",
                "minLength": 2,
                "maxLength": 50,
                "pattern": "^([a-zA-Z0-9_-]*)$"
            },
            "repository": {
                "type": "string",
                "pattern": "^(https://.*\\.git)$"
            }
        }
    }
}